Private Sub xMenu1_ItemClick(Key As String)
  Select Case Key
    Case "hFlat", "hSolid", "hLG", "hEG", "h3Dm", "h3Dw"
         
         xMenu1.ItemChecked("hFlat") = False
         xMenu1.ItemChecked("hSolid") = False
         xMenu1.ItemChecked("hLG") = False
         xMenu1.ItemChecked("hEG") = False
         xMenu1.ItemChecked("h3Dm") = False
         xMenu1.ItemChecked("h3Dw") = False
         
         xMenu1.ItemChecked(Key) = True
     
     Case "Off97": xMenu1.Style = 0
     Case "OffXP": xMenu1.Style = 1
     Case "Exp": xMenu1.Style = 2
     Case "OStep": xMenu1.Style = 3
     
  End Select

  
  Select Case Key
    Case "hFlat":      xMenu1.HighLightStyle = 0
    Case "hSolid":     xMenu1.HighLightStyle = 1
    Case "hLG":        xMenu1.HighLightStyle = 2
    Case "hEG":        xMenu1.HighLightStyle = 3
    Case "h3Dm":       xMenu1.HighLightStyle = 4
    Case "h3Dw":       xMenu1.HighLightStyle = 5
    
    Case "Off97", "OffXP", "Exp", "OStep"
       
       xMenu1.Refresh
    
  End Select
  
End Sub


